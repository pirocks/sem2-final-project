<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.welcomescreen.InvalidController">
   <center>
      <VBox>
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="You entered one or more invalid setting. Make sure that all numerical settings have a valid numerical setting." />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="If you chose to continue all invalid settings will be replaced by defaults Continue or go back to " />
         </children>
      </VBox>
   </center>
   <top>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Invalid Settings" BorderPane.alignment="CENTER">
         <font>
            <Font size="27.0" />
         </font>
      </Text>
   </top>
   <bottom>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="button" layoutX="154.0" layoutY="100.0" mnemonicParsing="false" onAction="#goBack" text="Go Back" />
            <Button layoutX="305.0" layoutY="100.0" mnemonicParsing="false" onAction="#continueNormally" text="Continue" />
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
